@{
    ViewData["Title"] = "Home Page";
}


@model Summoner


@using (Html.BeginForm("Search", "Home", "Get"))
{
    <label asp-for="SummonerName" class="control-label"style="color:white">League Of Legends</label>
    <input type="text" name="SummonerName" placeholder="Search for Summoner Name..." class="form-control" value="" />
    <input type="submit" value="Search" class="btn btn-primary" />
    <link rel="stylesheet" href="~/css/Searchbar.css">
}

